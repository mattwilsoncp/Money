<%= simple_form_for(@register_upload_entry) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :register_upload_id %>
    <%= f.input :entry_date %>
    <%= f.input :description %>
    <%= f.input :category_id %>
    <%= f.input :withdrawal %>
    <%= f.input :deposit %>
    <%= f.input :register_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
