<p id="notice"><%= notice %></p>

<h1>Register Upload Entries</h1>

<table>
  <thead>
    <tr>
      <th>Register upload</th>
      <th>Entry date</th>
      <th>Description</th>
      <th>Category</th>
      <th>Withdrawal</th>
      <th>Deposit</th>
      <th>Register</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @register_upload_entries.each do |register_upload_entry| %>
      <tr>
        <td><%= register_upload_entry.register_upload_id %></td>
        <td><%= register_upload_entry.entry_date %></td>
        <td><%= register_upload_entry.description %></td>
        <td><%= register_upload_entry.category_id %></td>
        <td><%= register_upload_entry.withdrawal %></td>
        <td><%= register_upload_entry.deposit %></td>
        <td><%= register_upload_entry.register_id %></td>
        <td><%= link_to 'Show', register_upload_entry %></td>
        <td><%= link_to 'Edit', edit_register_upload_entry_path(register_upload_entry) %></td>
        <td><%= link_to 'Destroy', register_upload_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Register Upload Entry', new_register_upload_entry_path %>
